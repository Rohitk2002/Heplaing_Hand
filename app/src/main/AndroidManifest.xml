<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rohitkashyap.HelpingHand">

    <application
        android:allowBackup="true"
        android:icon="@drawable/helping"
        android:label="@string/app_name"
        android:roundIcon="@drawable/helping"
        android:supportsRtl="true"
        android:theme="@style/Theme.Demo">
        <activity android:name="com.rohitkashyap.HelpingHand.Databases.RetrieveData"></activity>
        <activity android:name="com.rohitkashyap.HelpingHand.User.UserChoice" />
        <activity android:name="com.rohitkashyap.HelpingHand.Admin.ThankYou" />
        <activity android:name="com.rohitkashyap.HelpingHand.User.TakingInfo" />
        <activity android:name="com.rohitkashyap.HelpingHand.Common.LoginSignUp.ForgetPaswwordSuccessMessage" />
        <activity android:name="com.rohitkashyap.HelpingHand.Common.LoginSignUp.SetNewPassword" />
        <activity android:name="com.rohitkashyap.HelpingHand.Common.LoginSignUp.MakeSelection" />
        <activity android:name="com.rohitkashyap.HelpingHand.Common.LoginSignUp.ForgetPassword" />
        <activity android:name="com.rohitkashyap.HelpingHand.Common.LoginSignUp.SignUp2" />
        <activity android:name="com.rohitkashyap.HelpingHand.Common.LoginSignUp.SignUp" />
        <activity android:name="com.rohitkashyap.HelpingHand.Common.LoginSignUp.Login" />
        <activity
            android:name="com.rohitkashyap.HelpingHand.Common.OnBoarding"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name="com.rohitkashyap.HelpingHand.User.UserDashboard" />
        <activity android:name="com.rohitkashyap.HelpingHand.Common.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>